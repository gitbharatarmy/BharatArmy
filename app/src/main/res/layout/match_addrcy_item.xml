<?xml version="1.0" encoding="utf-8"?>
<layout>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/ticketLinear"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_70sdp"
        android:layout_marginLeft="@dimen/_5sdp"
        android:layout_marginTop="@dimen/_5sdp"
        android:layout_marginRight="@dimen/_5sdp"
        android:layout_marginBottom="@dimen/_5sdp"
        android:background="@drawable/travel_match_child_curveshape"
        android:orientation="horizontal"
        android:visibility="visible">

        <com.bharatarmy.RoundishImageView
            android:layout_width="@dimen/_70sdp"
            android:layout_height="@dimen/_70sdp"
            android:adjustViewBounds="true"
            android:scaleType="centerCrop"
            android:layout_margin="1dp"
            android:src="@drawable/australia_tour"
            app:cornerRadius="@dimen/round_corner_radius"
            app:roundedCorners="bottomLeft" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="1">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_weight="0.7"
                android:orientation="vertical">

                <TextView
                    style="@style/title_text_font"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:maxLines="2"
                    android:text="premium (bowler's arm - covered)"
                    android:textAllCaps="true"
                    android:textColor="@color/heading_bg"
                    android:textSize="@dimen/_12sdp" />

                <TextView
                    style="@style/title_text_font"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:gravity="center_horizontal|center_vertical"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:maxLines="2"
                    android:text="$265.00"
                    android:textColor="@color/heading_bg"
                    android:textSize="@dimen/_13sdp" />
                <TextView
                    style="@style/title_text_font"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="wrap_content"
                    android:gravity="right|center_vertical"
                    android:layout_marginBottom="@dimen/_5sdp"
                    android:text="per pax"
                    android:textAllCaps="true"
                    android:textColor="@color/orange"
                    android:textSize="@dimen/_5sdp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.3"
                android:orientation="vertical">


                <ImageView
                    android:id="@+id/ticketscart_addimage"
                    android:layout_width="@dimen/_13sdp"
                    android:layout_height="@dimen/_13sdp"
                    android:src="@drawable/ic_blank_check"
                    android:layout_gravity="right"
                    android:layout_marginTop="@dimen/_7sdp"
                    android:layout_marginRight="@dimen/_7sdp"/>

                <LinearLayout
                    android:id="@+id/addtoticketscart1Linear"
                    android:layout_width="@dimen/_27sdp"
                    android:layout_height="@dimen/_23sdp"
                    android:layout_gravity="right"
                    android:layout_marginTop="@dimen/_26sdp"
                    android:background="@drawable/match_cartshape"
                    android:visibility="visible">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_15sdp"
                        android:layout_gravity="center_vertical|center_horizontal"
                        android:src="@drawable/cart"
                        android:tint="@color/splash_bg_color" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/removetoticketscart1Linear"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginTop="@dimen/_30sdp"
                    android:background="@drawable/cart_removeshape"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <TextView
                        style="@style/title_text_font"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_20sdp"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="Remove"
                        android:textColor="@color/splash_bg_color"
                        android:textSize="@dimen/_10sdp" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/hotel_linear"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_5sdp"
        android:layout_marginTop="@dimen/_5sdp"
        android:layout_marginRight="@dimen/_5sdp"
        android:layout_marginBottom="@dimen/_5sdp"
        android:background="@drawable/travel_match_child_curveshape"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/hotel_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="1dp">

            <com.bharatarmy.RoundishImageView
                android:id="@+id/hotel_img"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_130sdp"
                android:adjustViewBounds="true"
                android:scaleType="centerCrop"
                android:src="@drawable/ajif"
                app:cornerRadius="@dimen/round_corner_radius"
                app:roundedCorners="topRight" />
            <View
                android:id="@+id/topView"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_below="@id/hotel_img"
                android:background="@drawable/bg_gradiant_line"
                android:visibility="visible" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/topView"
                android:background="@color/splash_bg_color"
                android:layout_marginLeft="@dimen/_7sdp"
                android:layout_marginRight="@dimen/_7sdp"
                android:layout_marginTop="@dimen/_3sdp"
                android:layout_marginBottom="@dimen/_7sdp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/_9sdp"
                        android:layout_weight="1">

                        <TextView
                            android:id="@+id/hotelname_txt"
                            style="@style/title_text_font"
                            android:layout_width="0dp"
                            android:layout_weight="0.9"
                            android:layout_height="wrap_content"
                            android:ellipsize="end"
                            android:maxLines="2"
                            android:text="PAN PACIFIC PERTH"
                            android:textColor="@color/heading_bg"
                            android:textSize="@dimen/_16sdp" />
                        <TextView
                            style="@style/title_text_font"
                            android:layout_width="0dp"
                            android:layout_weight="0.1"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right"
                            android:gravity="right|center_vertical"
                            android:text="per pax"
                            android:textAllCaps="true"
                            android:textColor="@color/orange"
                          android:layout_marginRight="@dimen/_5sdp"
                            android:textSize="@dimen/_6sdp" />

                    </LinearLayout>
                    <TextView
                        android:id="@+id/hotel_location_txt"
                        style="@style/title_text_font"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:text="@string/bharat_army_story"
                        android:textColor="@color/heading_bg"
                        android:textSize="@dimen/_9sdp"
                        android:visibility="gone" />

                    <com.whinc.widget.ratingbar.RatingBar
                        android:id="@+id/ratingBar"
                        android:layout_width="@dimen/_50sdp"
                        android:layout_height="@dimen/_16sdp"
                        app:rb_click_rating="true"
                        app:rb_count="3"
                        app:rb_empty="@drawable/empty"
                        app:rb_fill="@drawable/fill"
                        app:rb_max_count="5"
                        app:rb_space="0dp"
                        app:rb_touch_rating="true" />

                    <TextView
                        android:id="@+id/travel_cityhoteldesc_txt"
                        style="@style/text_font"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:ellipsize="end"
                        android:maxLines="2"
                        android:paddingBottom="@dimen/_3sdp"
                        android:text="Bharat Army Experience Package with Accommodation Stay."
                        android:textColor="@color/normal_font_color"
                        android:textSize="@dimen/_10sdp" />


                    <LinearLayout
                        android:id="@+id/select_room_linear"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/_3sdp"
                        android:layout_marginBottom="@dimen/_10sdp"
                        android:weightSum="1">

                        <com.bharatarmy.RoundishImageView
                            android:id="@+id/room_img"
                            android:layout_width="@dimen/_45sdp"
                            android:layout_height="@dimen/_45sdp"
                            android:adjustViewBounds="true"
                            android:scaleType="centerCrop"
                            android:src="@drawable/ajif"
                            app:cornerRadius="@dimen/round_image"
                            app:roundedCorners="all" />
                       <LinearLayout
                           android:layout_width="match_parent"
                           android:layout_height="@dimen/_44sdp"
                           android:orientation="vertical"
                           android:layout_marginLeft="@dimen/_7sdp">
                           <LinearLayout
                               android:layout_width="match_parent"
                               android:layout_height="wrap_content"
                               android:orientation="horizontal">
                               <ImageView
                                   android:layout_width="@dimen/_8sdp"
                                   android:layout_height="@dimen/_8sdp"
                                   android:src="@drawable/hotel"
                                   android:layout_gravity="center_vertical"
                                   android:tint="@color/unselected_view"/>
                               <TextView
                                   style="@style/title_text_font"
                                   android:layout_width="wrap_content"
                                   android:layout_height="wrap_content"
                                   android:gravity="center_vertical"
                                   android:text="@string/hotel_room"
                                   android:layout_marginLeft="@dimen/_2sdp"
                                   android:textColor="@color/unselected_view"
                                   android:paddingTop="@dimen/_3sdp"
                                   android:textSize="@dimen/_9sdp"
                                   android:visibility="visible"
                                   android:textAllCaps="true"/>
                           </LinearLayout>

                           <TextView
                               android:id="@+id/room_nametxt"
                               style="@style/title_text_font"
                               android:layout_width="match_parent"
                               android:layout_height="@dimen/_19sdp"
                               android:maxLines="1"
                               android:ellipsize="end"
                               android:gravity="center_vertical"
                               android:text="@string/hotel_room_type"
                               android:textColor="@color/heading_bg"
                               android:textSize="@dimen/_16sdp"
                               android:layout_marginTop="-2dp"
                               android:textAllCaps="true"/>

                           <TextView
                               style="@style/title_text_font"
                               android:layout_width="wrap_content"
                               android:layout_height="match_parent"
                               android:gravity="center_vertical"
                               android:layout_gravity="left"
                               android:text="@string/hotel_change"
                               android:layout_marginRight="@dimen/_7sdp"
                               android:textColor="@color/skip_color"
                               android:textSize="@dimen/_7sdp"
                               android:visibility="visible"
                               android:textAllCaps="true"/>
                       </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:visibility="gone">

                        <TextView
                            style="@style/title_text_font"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                        android:layout_gravity="center_horizontal"
                            android:text="+"
                            android:textStyle="bold"
                            android:textColor="@color/heading_bg"
                            android:textSize="@dimen/_25sdp"
                            android:textAllCaps="true"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/_7sdp"
                        android:layout_marginBottom="@dimen/_7sdp"
                        android:weightSum="1"
                        android:visibility="gone">

                        <com.bharatarmy.RoundishImageView
                            android:id="@+id/ticket_img"
                            android:layout_width="@dimen/_45sdp"
                            android:layout_height="@dimen/_45sdp"
                            android:adjustViewBounds="true"
                            android:scaleType="centerCrop"
                            android:src="@drawable/ajif"
                            app:cornerRadius="@dimen/round_image"
                            app:roundedCorners="all" />
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_marginLeft="@dimen/_7sdp">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <ImageView
                                    android:layout_width="@dimen/_12sdp"
                                    android:layout_height="@dimen/_12sdp"
                                    android:src="@drawable/silver_tickets"
                                    android:tint="@color/unselected_view"/>
                                <TextView
                                    style="@style/title_text_font"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center_vertical"
                                    android:text="@string/ticket_header"
                                    android:textColor="@color/unselected_view"
                                    android:layout_marginLeft="@dimen/_2sdp"
                                    android:paddingTop="@dimen/_3sdp"
                                    android:textSize="@dimen/_9sdp"
                                    android:visibility="visible"
                                    android:textAllCaps="true"/>

                                <TextView
                                    style="@style/title_text_font"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center_vertical"
                                    android:text="OR"
                                    android:textColor="@color/unselected_view"
                                    android:layout_marginLeft="@dimen/_5sdp"
                                    android:paddingTop="@dimen/_3sdp"
                                    android:textSize="@dimen/_9sdp"
                                    android:visibility="visible"
                                    android:textAllCaps="true"/>
                                <ImageView
                                    android:layout_width="@dimen/_12sdp"
                                    android:layout_height="@dimen/_12sdp"
                                    android:layout_marginLeft="@dimen/_5sdp"
                                    android:src="@drawable/hotel"
                                    android:tint="@color/unselected_view"/>
                                <TextView
                                    style="@style/title_text_font"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="center_vertical"
                                    android:text="@string/hospitality_header"
                                    android:textColor="@color/unselected_view"
                                    android:paddingTop="@dimen/_3sdp"
                                    android:layout_marginLeft="@dimen/_2sdp"
                                    android:textSize="@dimen/_9sdp"
                                    android:visibility="visible"
                                    android:textAllCaps="true"/>
                            </LinearLayout>


                            <TextView
                                style="@style/title_text_font"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center_vertical"
                                android:text="@string/hotel_hospitality"
                                android:textColor="@color/heading_bg"
                                android:textSize="@dimen/_16sdp"
                                android:layout_marginTop="-2dp"
                                android:textAllCaps="true"/>

                            <TextView
                                style="@style/title_text_font"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center_vertical"
                                android:layout_gravity="left"
                                android:text="@string/hotel_remove"
                                android:layout_marginRight="@dimen/_7sdp"
                                android:textColor="@color/unselected_view"
                                android:textSize="@dimen/_7sdp"
                                android:visibility="visible"
                                android:textAllCaps="true"/>
                        </LinearLayout>
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:layout_marginTop="@dimen/_1sdp"
                        android:background="@drawable/line_shape" />

                    <TextView
                        style="@style/title_text_font"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:paddingTop="@dimen/_5sdp"
                        android:paddingRight="@dimen/_7sdp"
                        android:paddingBottom="@dimen/_5sdp"
                        android:text="Amenities"
                        android:textColor="@color/unselected_view"
                        android:textSize="@dimen/_7sdp"
                        android:visibility="visible" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_27sdp"
                        android:orientation="horizontal"
                        android:weightSum="1"
                        android:visibility="visible">

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/amenities"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.5" />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <LinearLayout
                                    android:id="@+id/book_linear"
                                    android:layout_width="@dimen/_90sdp"
                                    android:layout_height="match_parent"
                                    android:layout_alignParentRight="true"
                                    android:background="@drawable/book_btn"
                                    android:orientation="horizontal"
                                    android:visibility="visible">

                                    <TextView
                                        android:id="@+id/book_txt"
                                        style="@style/text_font"
                                        android:layout_width="match_parent"
                                        android:layout_height="@dimen/_25sdp"
                                        android:layout_gravity="center_horizontal|center_vertical"
                                        android:layout_marginRight="@dimen/_2sdp"
                                        android:gravity="center_horizontal|center_vertical"
                                        android:text="Book Now"
                                        android:textColor="@color/splash_bg_color"
                                        android:textSize="@dimen/_12sdp"
                                        android:textStyle="normal" />

                                </LinearLayout>
                            </RelativeLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginTop="@dimen/_114sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:background="@drawable/pacakageprice_curve_shape"
                android:orientation="horizontal"
                android:visibility="visible">

                <TextView
                    android:id="@+id/packageprice_txt"
                    style="@style/title_text_font"
                    android:layout_width="@dimen/_90sdp"
                    android:layout_height="@dimen/_25sdp"
                    android:layout_gravity="center_horizontal|center_vertical"
                    android:layout_marginRight="@dimen/_2sdp"
                    android:gravity="center_horizontal|center_vertical"
                    android:paddingLeft="@dimen/_2sdp"
                    android:paddingTop="@dimen/_3sdp"
                    android:paddingRight="@dimen/_5sdp"
                    android:paddingBottom="@dimen/_2sdp"
                    android:text="â‚¹ 10000"
                    android:textColor="@color/splash_bg_color"
                    android:textSize="@dimen/_15sdp"
                    android:textStyle="bold" />

            </LinearLayout>

            <ImageView
                android:id="@+id/recommended_image"
                android:layout_width="@dimen/_35sdp"
                android:layout_height="@dimen/_50sdp"
                android:layout_alignParentRight="true"
                android:layout_marginTop="@dimen/_55sdp"
                android:layout_marginRight="@dimen/_7sdp"
                android:scaleType="centerInside"
                android:src="@drawable/bal"
                android:visibility="gone" />

        </RelativeLayout>


    </LinearLayout>
    <LinearLayout
        android:id="@+id/hospitalityLinear"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_150sdp"
        android:layout_marginLeft="@dimen/_5sdp"
        android:layout_marginTop="@dimen/_5sdp"
        android:layout_marginRight="@dimen/_5sdp"
        android:layout_marginBottom="@dimen/_5sdp"
        android:background="@drawable/travel_match_child_curveshape"
        android:orientation="horizontal"
        android:visibility="visible">

        <com.bharatarmy.RoundishImageView
            android:id="@+id/hospitalityMainImage"
            android:layout_width="@dimen/_100sdp"
            android:layout_height="@dimen/_150sdp"
            android:adjustViewBounds="true"
            android:scaleType="centerCrop"
           android:layout_margin="1dp"
            android:src="@drawable/australia_tour"
            app:cornerRadius="@dimen/round_corner_radius"
            app:roundedCorners="bottomLeft" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:weightSum="1">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_weight="0.7"
                android:orientation="vertical">

                <TextView
                    style="@style/title_text_font"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:maxLines="2"
                    android:text="VVIP Hospitality"
                    android:textAllCaps="true"
                    android:textColor="@color/heading_bg"
                    android:textSize="@dimen/_12sdp" />

                <TextView
                    style="@style/text_font"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:maxLines="5"
                    android:text="a flexible arrangement of seating and hospitality suites has been designed to anticipate fututre needs"
                    android:textColor="@color/normal_font_color"
                    android:textSize="@dimen/_9sdp" />
                <TextView
                    style="@style/title_text_font"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:maxLines="1"
                    android:text="Your Experience Includes"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_marginBottom="@dimen/_5sdp"
                    android:textColor="@color/heading_bg"
                    android:textSize="@dimen/_9sdp" />

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/exprienceRcv"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_20sdp"/>

                <TextView
                    style="@style/title_text_font"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="$265.00"
                    android:textColor="@color/heading_bg"
                    android:textSize="@dimen/_14sdp" />
                <TextView
                    style="@style/title_text_font"
                    android:layout_width="@dimen/_44sdp"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:layout_marginBottom="@dimen/_5sdp"
                    android:text="per pax"
                    android:textAllCaps="true"
                    android:textColor="@color/orange"
                    android:textSize="@dimen/_5sdp" />
            </LinearLayout>



            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.3"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/hospitality_addImage"
                    android:layout_width="@dimen/_13sdp"
                    android:layout_height="@dimen/_13sdp"
                    android:src="@drawable/ic_blank_check"
                    android:layout_gravity="right"
                    android:layout_marginTop="@dimen/_7sdp"
                    android:layout_marginRight="@dimen/_7sdp"/>

                <LinearLayout
                    android:id="@+id/addtohospitalitycart1Linear"
                    android:layout_width="@dimen/_27sdp"
                    android:layout_height="@dimen/_23sdp"
                    android:layout_gravity="right"
                    android:layout_marginTop="@dimen/_106sdp"
                    android:background="@drawable/match_cartshape"
                    android:visibility="visible">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="16dp"
                        android:layout_gravity="center_vertical|center_horizontal"
                        android:src="@drawable/cart"
                        android:tint="@color/splash_bg_color" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/removetohospitalitycart1Linear"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_marginTop="@dimen/_110sdp"
                    android:background="@drawable/cart_removeshape"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <TextView
                        style="@style/title_text_font"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_20sdp"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="Remove"
                        android:textColor="@color/splash_bg_color"
                        android:textSize="@dimen/_10sdp" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
</layout>