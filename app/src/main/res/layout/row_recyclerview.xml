<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_marginLeft="@dimen/_1sdp"
    android:layout_marginRight="@dimen/_1sdp"
    android:orientation="vertical">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/_5sdp"
        card_view:cardCornerRadius="1dp"
        card_view:cardElevation="2dp"
        card_view:cardMaxElevation="3dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/header_linear"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_23sdp"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_2sdp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/match_type_txt"
                    style="@style/title_text_font"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:background="@color/linear"
                    android:gravity="center_vertical|center"
                    android:padding="@dimen/_3sdp"
                    android:text="ODI"
                    android:textColor="@color/splash_bg_color"
                    android:textSize="@dimen/_9sdp"
                    android:visibility="visible" />
                <TextView
                    android:id="@+id/travel_date_txt"
                    style="@style/text_font"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center_vertical|center"
                    android:layout_marginLeft="@dimen/_2sdp"
                    android:text="Wed 05 Jun, 10:30 AM(Local)"
                    android:textColor="@color/normal_font_color"
                    android:textSize="@dimen/_10sdp"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/middle_linear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="@dimen/_3sdp">

                <TextView
                    android:id="@+id/first_match_txt"
                    style="@style/title_text_font"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center_vertical|center"
                    android:padding="@dimen/_3sdp"
                    android:text="India"
                    android:textColor="@color/heading_bg"
                    android:textSize="@dimen/_14sdp"
                    android:visibility="visible" />
                <ImageView
                    android:id="@+id/travel_flag_img"
                    android:layout_width="@dimen/_20sdp"
                    android:layout_height="@dimen/_20sdp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/in_flag"/>
                <TextView
                    android:id="@+id/travel_vs_txt"
                    style="@style/title_text_font"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center_vertical|center"
                    android:padding="@dimen/_3sdp"
                    android:text="vs"
                    android:textColor="@color/heading_bg"
                    android:textSize="@dimen/_14sdp"
                    android:visibility="visible"/>
                <ImageView
                    android:id="@+id/travel_flag1_img"
                    android:layout_width="@dimen/_20sdp"
                    android:layout_height="@dimen/_20sdp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/south_flag"/>
                <TextView
                    android:id="@+id/second_match_txt"
                    style="@style/title_text_font"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center_vertical|center"
                    android:padding="@dimen/_3sdp"
                    android:text="South Africa"
                    android:textColor="@color/heading_bg"
                    android:textSize="@dimen/_14sdp"
                    android:visibility="visible" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/third_linear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginBottom="@dimen/_3sdp">
                <ImageView
                    android:id="@+id/travel_location_img"
                    android:layout_width="@dimen/_9sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:src="@drawable/ic_location"
                    android:layout_gravity="center_vertical"/>
                <TextView
                    android:id="@+id/match_address_txt"
                    style="@style/text_font"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center_vertical|center"
                    android:text="Hampshire Bowl,Southampton"
                    android:textColor="@color/normal_font_color"
                    android:textSize="@dimen/_10sdp"
                    android:visibility="visible" />
            </LinearLayout>
        </LinearLayout>

    </androidx.cardview.widget.CardView>

