<?xml version="1.0" encoding="utf-8"?>
<!--remove the  code 25-02-2020 with backup-->

<resources>
    <declare-styleable name="VideoControllerView">
        <attr name="play_drawable" format="reference"/>
        <attr name="pause_drawable" format="reference"/>
        <attr name="enter_fullscreen_drawable" format="reference"/>
        <attr name="exit_fullscreen_drawable" format="reference"/>
        <attr name="ffwd_drawable" format="reference"/>
        <attr name="rew_drawable" format="reference"/>
        <attr name="progress_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="CountDownClock">
        <attr name="resetSymbol" format="string"/>
        <attr name="digitDividerColor" format="color"/>
        <attr name="digitTopDrawable" format="integer"/>
        <attr name="digitBottomDrawable" format="integer"/>
        <attr name="digitSplitterColor" format="color"/>
        <attr name="digitTextColor" format="color"/>
        <attr name="halfDigitHeight" format="dimension"/>
        <attr name="digitWidth" format="dimension"/>
        <attr name="digitTextSize" format="dimension"/>
        <attr name="digitPadding" format="dimension"/>
        <attr name="splitterPadding" format="dimension"/>
        <attr name="animationDuration" format="integer"/>
        <attr name="almostFinishedCallbackTimeInSeconds" format="integer"/>
        <attr name="countdownTickInterval" format="integer"/>
    </declare-styleable>
    <declare-styleable name="AlignedTextView">
        <attr name="alignment" format="integer"/>
    </declare-styleable>
    <declare-styleable name="CountryCodePicker">
        <attr format="integer" name="ccp_defaultCode"/>
        <attr format="color" name="ccp_textColor"/>
        <attr format="color" name="ccp_backgroundColor"/>
        <attr format="dimension" name="ccp_textSize"/>
        <attr format="string" name="ccp_textFont"/>
        <attr format="dimension" name="ccp_arrowSize"/>
        <attr format="string" name="ccp_countryPreference"/>
        <attr format="string" name="ccp_defaultNameCode"/>
        <attr format="boolean" name="ccp_hideNameCode"/>
        <attr format="boolean" name="ccp_keyboardAutoPopOnSearch"/>
        <attr format="string" name="ccp_customMasterCountries"/>
        <attr format="boolean" name="ccp_showFlag"/>
        <attr format="boolean" name="ccp_showFullName"/>
        <attr format="boolean" name="ccp_selectionDialogShowSearch"/>
        <attr format="boolean" name="ccp_clickable"/>
        <attr format="boolean" name="ccp_hidePhoneCode"/>
        <attr format="boolean" name="ccp_enableHint"/>
        <attr format="boolean" name="ccp_enablePhoneAutoFormatter"/>
        <attr format="boolean" name="ccp_setCountryByTimeZone"/>

        <!--For dialog-->
        <attr format="color" name="ccp_dialogTextColor"/>
    </declare-styleable>

    <declare-styleable name="RoundishImageView">
        <attr name="cornerRadius" format="dimension" />
        <attr name="roundedCorners">
            <flag name="topLeft" value="1" />
            <flag name="topRight" value="2" />
            <flag name="bottomRight" value="4" />
            <flag name="bottomLeft" value="8" />
            <flag name="all" value="15" />
        </attr>
    </declare-styleable>

    <declare-styleable
        name="Toggle">

        <attr
            name="textOn"
            format="string"/>

        <attr
            name="textOff"
            format="string"/>

        <attr
            name="colorOff"
            format="color"/>

        <attr
            name="colorOn"
            format="color"/>

        <attr
            name="colorBorder"
            format="color"/>

        <attr
            name="colorDisabled"
            format="color"/>

        <attr name="on"
            format="boolean"/>

        <attr name="android:textSize"/>

        <attr name="android:enabled"/>
    </declare-styleable>




</resources>